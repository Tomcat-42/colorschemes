## Base16 darkbox
# Author: Pablo Alessandro Santos Hugen
#
# You can use these variables anywhere in the mako configuration file.
sort=-time
layer=overlay
icons=0
max-icon-size=64
default-timeout=5000
ignore-timeout=1
font=Iosevka Nerd Font 12

background-color=#000000
text-color=#d5c4a1
border-color=#b8bb26

[urgency=low]
background-color=#000000
text-color=#d5c4a1
border-color=#b8bb26

[urgency=high]
background-color=#000000
text-color=#d5c4a1
border-color=#fb4934
